{
    "compilerOptions": {
        /* Language and Environment */
        "target": "esnext",
        /* Modules */
        "module": "commonjs",
        "resolveJsonModule": true,
        /* Interop Constraints */
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        /* Type Checking */
        "strict": true,
        /* Completeness */
        "skipLibCheck": true
    }
}